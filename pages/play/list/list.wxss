/* pages/play/list/list.wxss */
.container{
  position: fixed;
  bottom: 0;
  left: 0;
  right: 0;
  border-radius: 30rpx 30rpx 0 0 ;
  background: #fff;
  width: 100%;
}
.list-title{
  border-bottom: 1px solid #ededed;
  height: 80rpx;
  line-height: 60rpx;
  font-size: 40rpx;
  margin: 25rpx;
}
.panel-list scroll-view{
    height: 650rpx;
    border: 1px solid pink;
}
.contet-item {
  margin-left: 10rpx;
  font-size: 30rpx;
  height: 60rpx;
  border: 1px solid red;
  display: flex;
}
.list-image image{
  width: 40rpx;
  height: 40rpx;
}
